<?xml version="1.0" encoding="UTF-8" ?>
<!--银联智策-持卡人综合评分和衍生变量model-->
<step name="model" id="applyform_unionpaytransactionvariableb_model" status="able" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="../xsd/step.xsd">
    <relyon>applyform_unionpaytransactionvariableb_var,unionpaytransactionvariableb_var,applyform_var</relyon>
    <period>
        <periodInSecond>5</periodInSecond>
        <mainTable>model_score_score</mainTable>
        <params>
            <param name="gid" value="${req|gid}"/>
            <param name="model_id" value="${func|parse(${conf|applyform_unionpay_model_data_url})}"/>
        </params>
    </period>
    <handler type="http">
        <serviceCallingManager>
            <serviceCaller method="POST" paramForm="map" url="applyform_unionpay_model_data_url" timeThreshold="100"
                           failurePolicy="attempt">
                <mapper>
                    <mapping source="${req|gid}" target="gid"/>
                    <mapping source="${cxt|applyform_unionpaytransactionvariableb_var}" target="rid"/>
                    <mapping source="${unit|mid}" target="mid"/>
                    <mapping source="${unit|id}" target="cmid"/>
                </mapper>
            </serviceCaller>
            <attemptManager times="3" intervalInMilli="30"></attemptManager>
        </serviceCallingManager>
    </handler>
</step>